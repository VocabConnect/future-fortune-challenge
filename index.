// App State
let activeDeck = DECK.slice();
let availableIds = new Set(activeDeck.map(d => d.id));
let current = null;
let score = 0;
let timer = null;
let timerOn = false;

// UI Refs
const promptArea = document.getElementById('promptArea');
const promptTag = document.getElementById('promptTag');
const leftCount = document.getElementById('leftCount');
const scoreEl = document.getElementById('score');
const drawBtn = document.getElementById('drawBtn');
const correctBtn = document.getElementById('correctBtn');
const wrongBtn = document.getElementById('wrongBtn');
const nextBtn = document.getElementById('nextBtn');
const switchBtn = document.getElementById('switchBtn');
const filtersDiv = document.getElementById('filters');
const resetFilters = document.getElementById('resetFilters');
const shuffleAll = document.getElementById('shuffleAll');
const downloadBtn = document.getElementById('downloadBtn');
const resetScore = document.getElementById('resetScore');
const timerToggle = document.getElementById('timerToggle');
const timerSecInput = document.getElementById('timerSec');

// Constants for UI
const READY_TEXT = 'Ready';
const EMPTY_TEXT = 'No cards in the current filter. Reset filters or add categories.';

// Initialize the app
function initApp() {
  renderFilters();
  updateActiveDeck();
  scoreEl.textContent = score;
  leftCount.textContent = activeDeck.length;
  setupEventListeners();
}

// Render filter chips
function renderFilters() {
  filtersDiv.innerHTML = '';
  const cats = [...new Set(DECK.map(d => d.cat))];
  cats.forEach(cat => {
    const btn = document.createElement('button');
    btn.className = 'chip';
    btn.textContent = cat;
    btn.style.opacity = activeCats.has(cat) ? 1 : 0.45;
    btn.onclick = () => toggleFilter(cat);
    filtersDiv.appendChild(btn);
  });
}

// Toggle filter
let activeCats = new Set([...new Set(DECK.map(d => d.cat))]);
function toggleFilter(cat) {
  if (activeCats.has(cat)) {
    activeCats.delete(cat);
  } else {
    activeCats.add(cat);
  }
  updateActiveDeck();
  renderFilters();
}

// Update active deck based on filters
function updateActiveDeck() {
  activeDeck = DECK.filter(d => activeCats.has(d.cat));
  availableIds = new Set(activeDeck.map(d => d.id));
  leftCount.textContent = activeDeck.length;
}

// Pick a random card
function pickCard() {
  if (activeDeck.length === 0) {
    promptArea.textContent = EMPTY_TEXT;
    promptTag.textContent = 'Empty';
    return;
  }
  const idx = Math.floor(Math.random() * activeDeck.length);
  current = { ...activeDeck[idx] };
  promptArea.textContent = current.text;
  promptTag.textContent = current.cat;
}

// Remove current card from deck
function removeCurrentFromDeck() {
  if (!current) return;
  const idx = activeDeck.findIndex(d => d.id === current.id);
  if (idx >= 0) activeDeck.splice(idx, 1);
  current = null;
  promptArea.textContent = 'Press Draw card';
  promptTag.textContent = READY_TEXT;
  leftCount.textContent = activeDeck.length;
}

// Suggest a tense switch
function suggestSwitch(card) {
  const map = {
    'WILL': 'BE GOING TO',
    'BE GOING TO': 'PRESENT CONTINUOUS',
    'PRESENT CONTINUOUS': 'FUTURE CONTINUOUS',
    'FUTURE CONTINUOUS': 'FUTURE PERFECT',
    'FUTURE PERFECT': 'FUTURE PASSIVE',
    'FUTURE PASSIVE': 'MODAL FUTURE',
    'MODAL FUTURE': 'WILL',
    'MIX': 'WILL'
  };
  const target = map[card.cat] || 'WILL';
  return { cat: target, text: `(Reformulate) ${card.text} ‚Äî now answer using: ${target}` };
}

// Timer functions
function startTimer() {
  stopTimer();
  let sec = parseInt(timerSecInput.value) || 30;
  let t = sec;
  timerToggle.textContent = `‚è±Ô∏è ${t}s left`;
  timer = setInterval(() => {
    t--;
    timerToggle.textContent = `‚è±Ô∏è ${t}s left`;
    if (t <= 0) {
      clearInterval(timer);
      timer = null;
      timerToggle.textContent = '‚è±Ô∏è Time!';
      alert('Time! Encourage student to finish.');
    }
  }, 1000);
}

function stopTimer() {
  if (timer) {
    clearInterval(timer);
    timer = null;
    timerToggle.textContent = timerOn ? '‚è±Ô∏è Timer: On' : '‚è±Ô∏è Timer: Off';
  }
}

// Download HTML
function downloadHTML() {
  const html = `<!doctype html>
${document.documentElement.outerHTML}`;
  const blob = new Blob([html], { type: 'text/html' });
  const url = URL.createObjectURL(blob);
  const a = document.createElement('a');
  a.href = url;
  a.download = 'future-fortune-challenge.html';
  a.click();
  URL.revokeObjectURL(url);
}

// Setup event listeners
function setupEventListeners() {
  drawBtn.onclick = () => {
    pickCard();
    if (timerOn) startTimer();
  };
  
  correctBtn.onclick = () => {
    score++;
    scoreEl.textContent = score;
    if (document.getElementById('autoKeep').checked) {
      removeCurrentFromDeck();
    }
    stopTimer();
  };
  
  wrongBtn.onclick = () => {
    score = Math.max(0, score - 1);
    scoreEl.textContent = score;
    stopTimer();
  };
  
  nextBtn.onclick = () => {
    removeCurrentFromDeck();
    stopTimer();
  };
  
  switchBtn.onclick = () => {
    if (!current) return alert('Draw a card first.');
    const alt = suggestSwitch(current);
    promptArea.textContent = alt.text;
    promptTag.textContent = `${alt.cat} (SWITCH)`;
  };
  
  shuffleAll.onclick = () => {
    activeDeck = DECK.slice();
    renderFilters();
    updateActiveDeck();
    score = 0;
    scoreEl.textContent = 0;
    promptArea.textContent = 'Deck shuffled! Press Draw card.';
    leftCount.textContent = activeDeck.length;
  };
  
  resetFilters.onclick = () => {
    activeCats = new Set([...new Set(DECK.map(d => d.cat))]);
    renderFilters();
    updateActiveDeck();
  };
  
  resetScore.onclick = () => {
    score = 0;
    scoreEl.textContent = 0;
  };
  
  timerToggle.onclick = () => {
    timerOn = !timerOn;
    timerToggle.textContent = timerOn ? '‚è±Ô∏è Timer: On' : '‚è±Ô∏è Timer: Off';
    if (!timerOn) stopTimer();
  };
  
  downloadBtn.onclick = downloadHTML;
}

// Initialize the app
initApp();

    // Script will be added in the next step
  </script>
</body>
</html>
